      "auth": "noauth",
      "udp": true,
      "ip": "127.0.0.1"
    },
    "sniffing": {
      "enabled": true,
      "destOverride": ["http", "tls"]
    }
  }],

  "outbounds": [{
    "protocol": "vmess",
    "settings": {
      "vnext": [
          {
          "address": "xx.erbi.win",
          "port": 18765,
          "users": [
            {
              "id": "5e2b39d9-e9ca-49fb-a505-4933337fc7b2",
              "alterId": 64
            }
          ]
        }
      ]
    }
  }],

  "routing": {
    "domainStrategy": "IPOnDemand",
    "rules":[
      {
        "type": "field",
        "ip": ["geoip:private"],
        "outboundTag": "blocked"
      },
      {
        "type": "field",
        "domain": ["geosite:category-ads"],
        "outboundTag": "blocked"
      }
    ]
  },

  "dns": {
